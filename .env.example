# 環境變數範本
# 將此文件複製為 .env 並填入實際值

# 應用程式設定
APP_ENV=development
APP_DEBUG=true

# 資料庫配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=sleep_tracker
DB_USER=root
DB_PASS=

# API 配置
API_BASE_URL=http://localhost:8000
API_VERSION=v1

# Firebase 配置 (可選)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_DATABASE_URL=your_firebase_database_url
